fullGrammarEps : fullGrammar ;
fullGrammarEps : EPS ;

lowPriority : PLUS term1 lowPriority ;
lowPriority : MINUS term1 lowPriority ;
lowPriority : EPS ;

term1 : term2 highPriority;

highPriority : MULT value highPriority ;
highPriority : DIV value highPriority ;
highPriority : EPS ;

term2 : value power ;

power : POW value power ;
power : EPS ;

value : NUMBER;
value : VARIABLE;
value : LB term1 lowPriority RB;

PLUS  : "+" ;
MINUS : "-" ;
MULT  : "*" ;
DIV   : "/" ;
POW   : "^" ;
SEMI  : ";" ;
EQ    : "=" ;
LB    : "(" ;
RB    : ")" ;
